module choose_rx_ry(clk,IR,R0,R1,R2,R3,RX,RY);
	always@(posedge clk)
	begin
		case(IR[11:10])
			2'b00:begin
				RX<=R1;
				R1<=RX;
			end
			2'b01:begin
				RX<=R1;
				R1<=RX;
			end
			2'b10:begin
				RX<=R1;
				R1<=RX;
			end
			2'b11:begin
				RX<=R1;
				R1<=RX;
			end
		endcase
		case(IR[9:8])
			2'b00:begin
				RX<=R1;
				R1<=RX;
			end
			2'b01:begin
				RX<=R1;
				R1<=RX;
			end
			2'b10:begin
				RX<=R1;
				R1<=RX;
			end
			2'b11:begin
				RX<=R1;
				R1<=RX;
			end
		endcase
	end
endmodule
